{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
		"semi": [
            "error",
            "always"
        ]
    },
	"plugins": [
        "html"
    ]
}